		pRpMap++;

	}

	cbBasic = NULL;
	cbCache = NULL;
	cbModel = NULL;
	ce = NULL;
	ceCache = NULL;
	pfgCodebookCache = NULL;
	p = NULL;
	pfg = NULL;
	pfgCodebook = NULL;
	pbg = NULL;
	pDImage = NULL;
	pPreDImage = NULL;
	pDiffImage = NULL;


}
